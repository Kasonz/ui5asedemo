<mvc:View controllerName="sap.ui.demo.bulletinboard.controller.Worklist" 
	xmlns="sap.m" 
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns:semantic="sap.m.semantic">
	<semantic:FullscreenPage id="page" title="{i18n>worklistViewTitle}">
		<semantic:content>
			<FlexBox height="100px" alignItems="Start" justifyContent="Center" class="tddKataTitleContainer">
				<items>
					<Title class="tddKataTitle" text="{path: 'worklistView>/userName',formatter: '.formatter.greeter'}" level="H1" />
				</items>
			</FlexBox>
		</semantic:content>
	</semantic:FullscreenPage>
</mvc:View>
